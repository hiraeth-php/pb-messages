;
; CSS classmap provides the CSS classes used (values) for the type of message (keys)
;

classmap = {
	"ERROR": "error",
	"WARNING": "warning",
	"SUCCESS": "success",
	"INFO": "info"
}

[application]

	aliases = {
		"Plasticbrain\\FlashMessages\\FlashMessages": "Messaging"
	}

	providers = [
		"Hiraeth\\Messages\\MessagesProvider"
	]

[twig]

	globals = {
		"messaging": "Plasticbrain\\FlashMessages\\FlashMessages"
	}
